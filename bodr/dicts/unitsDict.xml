<unitList
  id="simpleUnits"
  namespace="http://www.xml-cml.org/units/units"
  dictionaryPrefix="units"
  title="Simple units dictionary"
  xmlns:units="http://www.xml-cml.org/units/units"
  siNamespace="http://www.xml-cml.org/units/siUnits"
  xmlns:si="http://www.xml-cml.org/units/siUnits"
  xmlns:unitType="http://www.xml-cml.org/units/unitTypes"
  xmlns="http://www.xml-cml.org/schema"
  xsi:schemaLocation="http://www.xml-cml.org/schema ../../schema.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
  <unit
      id="unknown"
      title="Unknown"
      parentSI="si:unknown"
      symbol="unk"
      unitType="unitType:unknown">
    <description>unknown unit and unknown unitType
    </description>
  </unit>

<!-- entries waiting for editing have dummy multipliers of -99 -->
  <unit
      id="ang"
      title="Angstrom" 
      parentSI="si:m"
      multiplierToSI="1.0E-10" 
      symbol="A" 
      unitType="unitType:length">
    <description>derived length unit
    </description>
  </unit>

  <unit
      id="ang-1"
      title="Angstrom-1"
      parentSI="si:m-1"
      multiplierToSI="1.0E10"
      symbol="A-1"
      unitType="unitType:length-1">
    <description>derived inverse length</description>
  </unit>

  <unit
      id="deg"
      title="degree"
      parentSI="si:radian"
      multiplierToSI="0.01745329"
      symbol="[deg]"
      unitType="unitType:angle">
    <description>common angle</description>
  </unit>

  <unit
      id="atmass"
      title="Atomic mass unit"
      parentSI="si:kg"
      multiplierToSI="1.6605402E-27"
      symbol="amu"
      unitType="unitType:mass">
    <description>
    </description>
  </unit>

  <unit
      id="ang3"
      title="Angstrom cubed"
      parentSI="si:m3"
      multiplierToSI="1.0E-30"
      symbol="A3"
      unitType="unitType:volume">
    <description>
    </description>
  </unit>

  <unit
      id="arb"
      title="Arbitrary"
      parentSI="si:arb"
      multiplierToSI="-99"
      symbol="arb"
      unitType="unitType:arbitrary">
    <description>
    </description>
  </unit>

